{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsx src/main/server.ts",
    "start:dev": "tsx watch src/main/server.ts",
    "build": "tsup src",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "vitest --passWithNoTests --no-threads",
    "test:unit": "vitest --passWithNoTests --no-threads --config ./vitest-unit.config.ts",
    "test:e2e": "dotenv -e .env.test -- npx prisma migrate deploy;dotenv -e .env.test -- vitest --no-threads --config ./vitest-e2e.config.ts --watch false",
    "test:coverage": "npm run test:e2e -- --coverage",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/terminal-kit": "^2.5.1",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "eslint": "^8.37.0",
    "git-commit-msg-linter": "^4.9.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prisma": "^4.12.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.3",
    "terminal-kit": "^3.0.0",
    "validator": "^13.9.0"
  }
}
